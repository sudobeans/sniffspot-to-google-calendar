{
  "manifest_version": 2,
  "name": "Sniffspot to Google Calendar",
  "version": "1.0",
  "description": "Converts Sniffspot reservations into a schedule that can be put in Google Calendar.",
  "icons": {
    "48": "icons/logo-320.png"
  },

  "content_scripts": [
    {
      "matches": [
        "*://*.sniffspot.com/*"
      ],
      "js": [
        "main.js"
      ]
    }
  ],

  "browser_action": {
    "default_icon": {
      "38": "icons/logo-320.png"
    }
  }
}